<%# pop the *first* link and treat that one special.  -%>
<% tags = links.keys
   tag0 = tags.pop
   files = links[tag0].keys
   file0 = files.pop 
-%>
 "<%= file0 -%>" : { "target" : "<%= folders[tag0] -%>"<% if links[tag0][file0] -%>, "exts" : [ "<%- l_exts = links[tag0][file0].sort -%>
